from Crypto.Util.number import long_to_bytes

def franklinReiter(n, e, r, c1, c2):
    R.<X> = Zmod(n)[]
    f1 = X^e - c1
    f2 = (X + r)^e - c2
    return Integer(n - (pgcd(f1, f2)).coefficients()[0])

def pgcd(g1, g2):
    return g1.monic() if not g2 else pgcd(g2, g1%g2)

